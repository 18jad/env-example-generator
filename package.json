{
  "name": "enveg",
  "repository": "github:18jad/enveg",
  "version": "0.0.12",
  "description": "A simple generator to generate environment variables example file.",
  "main": "dist/index.js",
  "bin": {
    "enveg": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node index.ts",
    "start:dev": "nodemon --watch . --exec ts-node index.ts",
    "build": "rm -rf dist/ && npm run build:esm",
    "build:esm": "tsc"
  },
  "keywords": [
    "env",
    ".env",
    "environment",
    "variables",
    "example",
    "generator",
    "enveg",
    ".env.example",
    "dotenv"
  ],
  "author": "Jad Yahya",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": ">=2.7"
  },
  "devDependencies": {
    "@types/node": "^18.11.11"
  }
}
